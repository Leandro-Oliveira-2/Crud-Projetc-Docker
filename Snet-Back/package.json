{
   "name": "Snet-Back",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/figlet": "^1.5.4",
      "@types/fs-extra": "^9.0.13",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/mime": "^2.0.3",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.10.0",
      "@typescript-eslint/parser": "^5.10.0",
      "eslint": "^7.32.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-typescript": "^16.1.0",
      "eslint-import-resolver-typescript": "^2.5.0",
      "eslint-plugin-import": "^2.25.4",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^3.12.0",
      "tscpaths": "^0.0.9",
      "typescript": "4.5.2"
   },
   "scripts": {
      "build": "tsc -p . && tscpaths -p tsconfig.json -s ./src -o ./dist",
      "db:migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
      "db:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
      "db:migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
      "run:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/common/infra/http/server.ts",
      "run:prod": "node -r tsconfig-paths/register dist/common/infra/http/server.js"
   },
   "dependencies": {
      "@types/axios": "^0.14.0",
      "@types/ejs": "^3.1.0",
      "@types/html-pdf": "^3.0.0",
      "argon2": "^0.28.3",
      "axios": "^0.27.2",
      "chalk": "^4.1.2",
      "child_process": "^1.0.2",
      "class-transformer": "^0.5.1",
      "cors": "^2.8.5",
      "csvtojson": "^2.0.10",
      "dotenv": "^14.2.0",
      "ejs": "^3.1.7",
      "express": "^4.17.2",
      "express-async-errors": "^3.1.1",
      "figlet": "^1.5.2",
      "fs": "^0.0.1-security",
      "fs-extra": "^10.1.0",
      "html-pdf": "^3.0.1",
      "http-status-codes": "^2.2.0",
      "https": "^1.0.0",
      "inversify": "^6.0.1",
      "jsonwebtoken": "^9.0.0",
      "mime": "^3.0.0",
      "multer": "^1.4.4",
      "mysql": "^2.18.1",
      "path": "^0.12.7",
      "pg": "^8.7.3",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.3.0",
      "typeorm": "^0.2.41",
      "uuid": "^8.3.2",
      "zod": "^3.11.6"
   }
}
